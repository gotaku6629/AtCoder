    "s = input()\n",
    "t = \"\"\n",
    "for i in range(len(s)): # N(10^{5})\n",
    "    if s[i] == 'R':\n",
    "        t = t[::-1]\n",
    "    else:\n",
    "        t = t + s[i]\n",
    "#print(t)\n",
    "\n",
    "i = 0\n",
    "while(i < len(t)-1):\n",
    "    #print(i,t)\n",
    "    if t[i] == t[i+1]:\n",
    "        t = t[:i]+t[i+2:]\n",
    "        i -= 1\n",
    "    else:\n",
    "        i += 1\n",
    "print(t)"
